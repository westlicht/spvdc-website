{
  "private": true,
  "name": "spvdc",
  "homepage": "http://www.swiss-pvd-coating.ch/",
  "locales": [
    "de",
    "fr",
    "en"
  ],
  "config": {
    "googleMaps": {
      "key": "AIzaSyBYyj0vwPlxPakSIxBHMKkLUtrJpg1TF2s"
    },
    "googleAnalytics": {
      "ua": "UA-90449204-1",
      "debug": false
    }
  },
  "twitter": "",
  "repository": "git@github.com:westlicht/spvdc-website.git",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build",
    "create-vcards": "node scripts/create-vcards.js",
    "#optimize": "node scripts/optimize-images.js",
    "prebuild": "npm run create-vcards",
    "postbuild": "npm run optimize",
    "deploy": "npm run build && scripts/deploy-belele.sh"
  },
  "phenomic": {},
  "#babel": "webpack-(development|production) are useful for webpack 2, otherwise use development|production",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-react-optimize",
          "babel-preset-latest",
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      },
      "webpack-development": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ],
        "plugins": [
          "babel-plugin-transform-react-jsx-source",
          "babel-plugin-transform-react-jsx-self",
          "react-hot-loader/babel"
        ]
      },
      "webpack-production": {
        "presets": [
          "babel-preset-react-optimize",
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-stage-1",
          "babel-preset-react"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "./node_modules/phenomic/lib/eslint-config-recommended/index.js"
  },
  "stylelint": {
    "extends": "./node_modules/phenomic/lib/stylelint-config-recommended/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.4.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "flat": "^2.0.1",
    "font-awesome": "^4.7.0",
    "history": "^2.1.2",
    "imagemin": "^5.3.1",
    "imagemin-mozjpeg": "^6.0.0",
    "intl": "^1.2.5",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.4",
    "lower-case": "^1.1.3",
